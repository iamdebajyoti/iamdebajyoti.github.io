{
    "manifest_version": 2,
    "name": "Borderify",
    "version": "1.0.0",
    "content_scripts": [
        {
            "matches": ["*://*.mozilla.org/*","*://*.google.com/*"],
            "js": ["borderify.js"]
        }
    ],
    "icons": {
        "48": "icons/border_96.png"
    },

    "description": " Add a red-border to all pages of mozilla.org"
}